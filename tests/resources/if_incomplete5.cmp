(:indentation
"subroutine foo()
     if (condition) then
          call foo()
     else
          return
          ! comment
     "

:font-lock
((1 1 11 font-lock-keyword-face) (1 12 15 font-lock-function-name-face) (1 15 17 f90-ts-font-lock-bracket-face) (2 23 25 font-lock-keyword-face) (2 26 27 f90-ts-font-lock-bracket-face) (2 36 37 f90-ts-font-lock-bracket-face) (2 38 42 font-lock-keyword-face) (3 53 57 font-lock-keyword-face) (3 58 61 font-lock-function-name-face) (3 61 63 f90-ts-font-lock-bracket-face) (4 69 73 font-lock-keyword-face) (5 84 90 font-lock-keyword-face) (6 101 110 font-lock-comment-face))

:tree
"(translation_unit (ERROR (subroutine_statement name: (name)) (parenthesized_expression (identifier)) (subroutine_call subroutine: (identifier) (argument_list)) (else_clause (keyword_statement)) (comment)))")
